{
  "expo": {
    "name": "movierank",
    "description": "Universal React App (iOS, Android, Web) to discover movies and series.",
    "slug": "movierank",
    "privacy": "public",
    "sdkVersion": "36.0.0",
    "platforms": ["ios", "android", "web"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",

    "backgroundColor": "#000000",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.geoniljang.rank",
      "buildNumber": "1.0.0"
    },
    "android": {
      "package": "com.geoniljang.rank",
      "versionCode": 1
    },
    "androidStatusBar": {
      "backgroundColor": "#000000"
    },
    "androidNavigationBar": {
      "backgroundColor": "#000000"
    }
  }
}
